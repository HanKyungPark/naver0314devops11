package day0322;

class Ex9_Constructor
{
	private String name;
	private int kor,eng,tot;
	private double avg;
	
	Ex9_Constructor()
	{
		this("이름미정",20,20);//반드시 첫줄이어야만 한다
	}
	
	Ex9_Constructor(String name)
	{
		this(name,50,60);
	}
	
	Ex9_Constructor(String name,int kor,int eng)
	{
		this.name=name;
		this.kor=kor;
		this.eng=eng;
		this.tot=kor+eng;
		this.avg=tot/2.0;
	}
}
public class Ex9_Constructor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
